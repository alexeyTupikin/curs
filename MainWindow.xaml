<Window x:Class="curs.MainWindow" Icon="img/icon.png"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:curs"
        mc:Ignorable="d"
        Title="Генератор степендионных ведомостей (Гость)" Height="450" Width="800" ResizeMode="CanMinimize"
        Background="#FFD2C8BE">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="one" Width="120*"/>
            <ColumnDefinition Width="680*"/>
        </Grid.ColumnDefinitions>
        <Image Grid.Column="0" Source="img/icon.png" Margin="5,5,5,310"/>
        <Canvas Grid.Column="1">
            <Frame Grid.Column="1" x:Name="frame" Height="390" Width="650" Margin="10,10,10,10" NavigationUIVisibility="Hidden"/>
            <Button x:Name="go_back"  Style="{StaticResource ButtonTheme}" Click="go_back_Click" Content="Назад" Canvas.Left="603" Canvas.Top="10" Width="57" Height="24" Background="#FFCBCBB6"/>
        </Canvas>
    </Grid>
</Window>

